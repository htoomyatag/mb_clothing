        <div id="content" class="<%= !content_for?(:sidebar) ? "row" : "col-sm-8 col-md-9" %>" data-hook>
          
          <%= flash_messages %>
          <br><br><br>
          <%= yield %>
        </div>

        <%= yield :templates %>
      </div>
    </div>
